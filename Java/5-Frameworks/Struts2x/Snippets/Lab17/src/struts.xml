<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.devMode" value="true" />  
    <constant name="struts.action.extension" value="jlc"/> 
    <package name="ganesh" namespace="/ganesh" extends="struts-default">
    	<action name="TestDisplay" class="com.ganesh.struts2.TestAction">
    	<result>/test.jsp</result>
    	</action>
    
    	<action name="RegisterSubmit" class="com.ganesh.struts2.RegisterSubmitAction">
    		<result name="success">/regStatus.jsp</result>
    	</action>
    </package>    
</struts>
