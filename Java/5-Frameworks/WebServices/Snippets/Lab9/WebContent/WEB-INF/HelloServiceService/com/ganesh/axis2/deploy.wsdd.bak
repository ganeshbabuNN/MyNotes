<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from HelloServiceService WSDL service -->

  <service name="HelloService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://axis2.ganesh.com"/>
      <parameter name="wsdlServiceElement" value="HelloServiceService"/>
      <parameter name="schemaQualified" value="http://axis2.ganesh.com"/>
      <parameter name="wsdlServicePort" value="HelloService"/>
      <parameter name="className" value="com.ganesh.axis2.HelloServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="HelloService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getMessage" qname="operNS:getMessage" xmlns:operNS="http://axis2.ganesh.com" returnQName="retNS:getMessageReturn" xmlns:retNS="http://axis2.ganesh.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://axis2.ganesh.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getMessage"/>

  </service>
</deployment>
