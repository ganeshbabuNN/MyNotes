<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
    
<application>
	<locale-config>
		<default-locale>en</default-locale>
	</locale-config>
	<message-bundle>com.ganesh.jsf.messages</message-bundle>
	 <view-handler>org.ajax4jsf.application.AjaxViewHandler</view-handler>
</application>
<managed-bean>
<managed-bean-name>studentBean</managed-bean-name>
<managed-bean-class>com.ganesh.jsf.StudentBean</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
<managed-bean-name>batchBean</managed-bean-name>
<managed-bean-class>com.ganesh.jsf.BatchBean</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<navigation-rule>
	<from-view-id>/register.jsp</from-view-id>
	<navigation-case>
		<from-outcome>success</from-outcome>
		<to-view-id>/regstatus.jsp</to-view-id>
	</navigation-case>
</navigation-rule>
</faces-config>
