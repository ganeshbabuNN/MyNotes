<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">

<converter>
	<converter-id>bidconverter</converter-id>
	<converter-class>com.ganesh.jsf.BIDConverter</converter-class>
</converter>
<converter>
	<converter-id>sidconverter</converter-id>
	<converter-class>com.ganesh.jsf.SIDConverter</converter-class>
</converter>
<managed-bean>
	<managed-bean-name>studentBean</managed-bean-name>
	<managed-bean-class>com.ganesh.jsf.StudentBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<application>
	<message-bundle>com.ganesh.jsf.messages</message-bundle>
</application>

<navigation-rule>
	<from-view-id>/register.jsp</from-view-id>
	<navigation-case>
		<from-outcome>regstatus</from-outcome>
		<to-view-id>/regstatus.jsp</to-view-id>
	</navigation-case>
</navigation-rule>
</faces-config>
